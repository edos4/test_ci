<div id="<%= dom_id email %>">
  <p>
    <strong>Subject:</strong>
    <%= email.subject %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= email.body %>
  </p>

  <p>
    <strong>Sender:</strong>
    <%= email.sender %>
  </p>

  <p>
    <strong>Recipient:</strong>
    <%= email.recipient %>
  </p>

</div>
